{
  "name": "deelan",
  "version": "0.1.0",
  "private": true,
  "description": "Data Engineering Electronic LAboratory Notebook",
  "type": "module",
  "bin": {
    "deelan": "./bin/deelan.mjs"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build:prepare-mathjax": "tsx scripts/prepare-mathjax.ts",
    "build:sync-search-core": "tsx scripts/sync-search-core.ts",
    "build:indexes": "tsx scripts/build-indexes.ts",
    "build:timeline": "tsx scripts/build-git-timeline.ts",
    "build:preflight": "npm run build:prepare-mathjax && npm run build:sync-search-core && npm run validate && npm run build:indexes && npm run build:timeline",
    "validate": "node ./bin/deelan.mjs validate",
    "dev": "npm run build:prepare-mathjax && npm run build:sync-search-core && astro dev",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "tags": "node ./bin/deelan.mjs tags",
    "docs:install": "python3 -m pip install -r requirements-docs.txt",
    "docs:serve": "mkdocs serve -w docs/ -w mkdocs.yml --livereload",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy --force",
    "prebuild": "npm run build:preflight",
    "build": "astro build",
    "serve": "astro preview",
    "preview": "npm run serve",
    "export": "node ./bin/deelan.mjs export",
    "storybook:serve": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build --output-dir .site-storybook"
  },
  "dependencies": {
    "astro": "^5.2.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "marked": "^17.0.3",
    "marked-highlight": "^2.2.3",
    "mathjax": "^4.1.0",
    "playwright": "^1.58.2",
    "shiki": "^3.22.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/builder-vite": "^10.2.10",
    "storybook": "^10.2.10",
    "storybook-astro": "^0.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
