{
  "name": "deelan",
  "version": "0.1.0",
  "private": true,
  "description": "Data Engineering Electronic LAboratory Notebook",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "prepare:mathjax": "tsx scripts/prepare-mathjax.ts",
    "sync:search-core": "tsx scripts/sync-search-core.ts",
    "dev": "npm run prepare:mathjax && npm run sync:search-core && astro dev",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "tags": "tsx scripts/tags.ts",
    "validate": "tsx scripts/validate-content.ts",
    "docs:install": "python3 -m pip install -r requirements-docs.txt",
    "docs:serve": "mkdocs serve -w docs/ -w mkdocs.yml --livereload",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy --force",
    "prebuild": "npm run prepare:mathjax && npm run sync:search-core && npm run validate && tsx scripts/build-indexes.ts && tsx scripts/build-git-timeline.ts",
    "build": "astro build",
    "preview": "astro preview",
    "export": "tsx scripts/export-item.ts",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build --output-dir .site-storybook"
  },
  "dependencies": {
    "astro": "^5.2.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "js-yaml": "^4.1.1",
    "marked": "^17.0.3",
    "marked-highlight": "^2.2.3",
    "mathjax": "^4.1.0",
    "playwright": "^1.58.2",
    "shiki": "^3.22.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^10.2.10",
    "@storybook/builder-vite": "^10.2.10",
    "storybook": "^10.2.10",
    "storybook-astro": "^0.1.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
