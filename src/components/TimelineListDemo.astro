---
import TimelineItem from './TimelineItem.astro';

interface Props {
  commit: string;
  commitUrl?: string;
  date: string;
  message: string;
  author: string;
}

const { commit, commitUrl, date, message, author } = Astro.props;
---

<ul class="timeline-list" style="padding: 0; margin: 0;">
  <TimelineItem commit={commit} commitUrl={commitUrl} date={date} message={message} author={author} />
</ul>
